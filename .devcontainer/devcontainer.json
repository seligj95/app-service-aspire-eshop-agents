{
  "name": "Aspire Eshop Agents Dev Container",
  "build": {
    "dockerfile": "Dockerfile"
  },
  "features": {},
  "customizations": {
    "vscode": {
      "settings": {},
      "extensions": [
        "ms-dotnettools.csharp",
        "ms-azuretools.vscode-azurecli",
        "ms-python.python",
        "ms-azuretools.vscode-bicep"
      ]
    }
  },
  "forwardPorts": [5000, 5001],
  "postCreateCommand": "pip install -r requirements.txt && dotnet --info && azd --version && az --version"
}
